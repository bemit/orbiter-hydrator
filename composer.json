{
    "name": "orbiter/hydrator",
    "license": "MIT",
    "homepage": "https://bemit.codes",
    "version": "0.0.1",
    "description": "PHP Object Hydrator using Reflection with PSR Container support.",
    "require": {
        "php": "~8.0||~8.1",
        "psr/container": "^1.0|^2.0",
        "orbiter/annotations-util": "~0.5.0"
    },
    "autoload": {
        "psr-4": {
            "Orbiter\\Hydrator\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "php-di/php-di": "^6.3.5",
        "phpunit/phpunit": "^9.5"
    }
}
